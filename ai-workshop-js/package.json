{
  "name": "code-dependency-visualizer",
  "version": "1.0.0",
  "description": "An interactive tool for visualizing code dependencies in JavaScript/TypeScript projects",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install"
  },
  "keywords": [
    "dependencies",
    "visualization",
    "d3",
    "code-analysis"
  ],
  "author": "AI Workshop",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "nodemon": "^3.0.1",
    "playwright": "^1.54.1"
  }
}
